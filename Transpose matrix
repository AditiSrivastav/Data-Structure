#include <stdio.h> 
int main() {    
     int matrix[50][50];     
     int r,c; 
      
    printf("Enter the number of rows: ");     
    scanf("%d",&r); 
    printf("Enter the number of columns: ");   
      scanf("%d",&c); 
  
    printf("\nEnter matrix elements :\n");     
    for(int i=0;i< r;i++) 
    {         for(int j=0;j< c;j++) 
        { 
            scanf("%d",&matrix[i][j]); 
        } 
    } 
    printf("\nEntered Matrix:\n");     
    for(int i=0;i< r;i++) 
    {         for(int j=0;j< c;j++) 
        {           
              printf("%d\t",matrix[i][j]);     
        }         printf("\n");   
    } 
    printf("\nTranspose Matrix:\n");   
      for(int i=0;i< c;i++) 
 
    {         for(int j=0;j< r;j++) 
        {          
               printf("%d\t",matrix[j][i]);     
        }       
          printf("\n");     
    }    
         return 0;        
} 

